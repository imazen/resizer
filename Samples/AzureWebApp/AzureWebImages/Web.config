<configuration>
    <configSections>
        <section name="resizer" type="ImageResizer.ResizerSection" />
    </configSections>

    <resizer>
        <plugins>
            <!-- Choose the version of AzureReader that matches the Azure SDK you have. -->
            
            <!-- Azure 1.x -->
            <add name="AzureReader" connectionString="BlobConn" blobStorageEndpoint="http://127.0.0.1:10000/devstoreaccount1/" />

            <!-- Azure 2.x -->
            <!--<add name="AzureReader2" connectionString="BlobConn" blobStorageEndpoint="http://127.0.0.1:10000/devstoreaccount1/" />-->
        </plugins>
    </resizer>

    <system.diagnostics>
        <trace>
            <listeners>
                <!-- Choose the version of DiagnosticMonitorTraceListener that matches the Azure SDK you have. -->
                
                <!-- Azure 1.x -->
                <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
                    <filter type="" />
                </add>

                <!-- Azure 2.x -->
                <!--<add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.2.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
                    <filter type="" />
                </add>-->
            </listeners>
        </trace>
    </system.diagnostics>

    <system.web>
        <compilation debug="true" targetFramework="4.0" />
        <httpModules>
            <add name="ImageResizingModule" type="ImageResizer.InterceptModule" />
        </httpModules>
        <machineKey decryption="AES" decryptionKey="0CA3EFAF0F7A5E7A62681C0BF656EE0ECE31ACEE3E1023BA3FAD20EA5F199DE8" validation="SHA1" validationKey="5D77574BF218C91090E4B346F1E75AA805867E0E9DE62168764FF0DCE537184F0535D5D9AD66DEDC97DC1ABFF7FA540B4DFD82E5BB196B95D15FF81F75AD5328" />
    </system.web>

    <system.webServer>
        <validation validateIntegratedModeConfiguration="false" />
        <modules>
            <add name="ImageResizingModule" type="ImageResizer.InterceptModule" />
        </modules>
    </system.webServer>
</configuration>